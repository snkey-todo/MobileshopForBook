<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mobileshop.mobile" android:versionCode="110" android:versionName="1.1">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/javashop_logo"
        android:label="@string/app_name"
        android:name=".basic.MyApplication"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.mobileshop.mobile.SplashActivity"
            android:label="@string/app_name"
            android:launchMode="singleTask"
            android:theme="@style/SplashTheme" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.mobileshop.mobile.AdActivity"
            android:launchMode="singleInstance" android:noHistory="true" android:screenOrientation="portrait">
        </activity>
        <activity android:name="com.mobileshop.mobile.MainActivity" android:launchMode="singleTask" android:screenOrientation="portrait"/>
        <activity android:name="com.mobileshop.mobile.LoginActivity" android:screenOrientation="portrait"/>
        <activity android:name="com.mobileshop.mobile.RegisterActivity" android:screenOrientation="portrait"/>
        <activity android:name="com.mobileshop.mobile.SelectAddressActivity" android:screenOrientation="portrait"/>
        <activity android:name="com.mobileshop.mobile.AddressModifyActivity" android:screenOrientation="portrait"/>
        <activity android:name="com.mobileshop.mobile.AddressActivity" android:screenOrientation="portrait"/>
        <activity android:name="com.mobileshop.mobile.CheckoutActivity" android:windowSoftInputMode="adjustPan" android:screenOrientation="portrait"/>
        <activity android:name="com.mobileshop.mobile.PaymentDeliveryActivity" android:screenOrientation="portrait"/>
        <activity android:name="com.mobileshop.mobile.ReceiptActivity" android:windowSoftInputMode="adjustPan" android:screenOrientation="portrait"/>
        <activity android:name="com.mobileshop.mobile.OrderSuccessActivity" android:screenOrientation="portrait"/>
        <activity android:name="com.mobileshop.mobile.SearchActivity" android:screenOrientation="portrait"/>
        <activity android:name="com.mobileshop.mobile.MyOrderActivity" android:screenOrientation="portrait"/>
        <activity android:name="com.mobileshop.mobile.MyOrderDetailActivity" android:screenOrientation="portrait"/>
        <activity android:name="com.mobileshop.mobile.MyFavoriteActivity" android:screenOrientation="portrait"/>
        <activity android:name="com.mobileshop.mobile.GoodsActivity" android:screenOrientation="portrait"/>
        <activity android:name="com.mobileshop.mobile.GoodsDetailActivity" android:screenOrientation="portrait"/>
        <activity android:name="com.mobileshop.mobile.GoodsCommentActivity" android:screenOrientation="portrait"/>
        <activity android:name="com.mobileshop.mobile.CartActivity" android:screenOrientation="portrait"/>
        <activity android:name="com.mobileshop.mobile.GoodsListActivity" android:screenOrientation="portrait"/>
        <activity android:name="com.mobileshop.mobile.MyAccountActivity" android:screenOrientation="portrait"/>
        <activity android:name="com.mobileshop.mobile.payment.AlipayMobilePaymentActivity" android:screenOrientation="portrait"/>
        <activity android:name="com.mobileshop.mobile.payment.WechatMobilePaymentActivity" android:screenOrientation="portrait"/>
        <activity
            android:name="com.alipay.sdk.app.H5PayActivity" android:configChanges="orientation|keyboardHidden|navigation" android:exported="false"
            android:screenOrientation="behind" >
        </activity>
        <activity
            android:name="com.alipay.sdk.auth.AuthActivity" android:configChanges="orientation|keyboardHidden|navigation" android:exported="false"
            android:screenOrientation="behind" >
        </activity>

        <activity android:name="com.mobileshop.mobile.AboutActivity" android:screenOrientation="portrait"/>
        <activity android:name="com.mobileshop.mobile.ImageViewerActivity" android:screenOrientation="portrait"/>
        <activity android:name="com.mobileshop.mobile.wxapi.WXPayEntryActivity" android:exported="true" android:label="@string/app_name"/>
        <activity android:name="com.mobileshop.mobile.MobileRegisterActivity1" android:screenOrientation="portrait"/>
        <activity android:name="com.mobileshop.mobile.MobileRegisterActivity2" android:screenOrientation="portrait"/>
        <activity android:name="com.mobileshop.mobile.MobileRegisterActivity3" android:screenOrientation="portrait"/>
        <activity android:name="com.mobileshop.mobile.FindPasswordActivity1" android:screenOrientation="portrait"/>
        <activity android:name="com.mobileshop.mobile.FindPasswordActivity2" android:screenOrientation="portrait"/>
        <activity android:name="com.mobileshop.mobile.FindPasswordActivity3" android:screenOrientation="portrait"/>
        <activity android:name="com.mobileshop.mobile.StoreAllGoodsActivity" android:screenOrientation="portrait" />
        <activity android:name="com.mobileshop.mobile.StoreDetailActivity" android:screenOrientation="portrait" /><!-- ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information. -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
    </application>

</manifest>
